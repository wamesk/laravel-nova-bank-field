{
    "name": "wamesk/laravel-nova-bank-field",
    "description": "Laravel Nova 4 Bank field.",
    "keywords": [
        "laravel",
        "nova",
        "nova4",
        "bank",
        "iban",
        "bban",
        "bic"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Wame\\Bank\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\Bank\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
